<Login>:
    MDLabel:
        text: 'Login'
        pos_hint: {'center_y': .85}
        font_style: 'H4'
        halign: 'center'
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
    MDLabel:
        text: 'Welcome to my house'
        pos_hint: {'center_y': .75}
        font_style: 'H5'
        halign: 'center'
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
    MDTextField:
        id: id_user_name
        hint_text: 'Enter your email'
        pos_hint: {'center_x': .5,'center_y': .6}
        current_hint_text_color: 0,0,0,1
        size_hint_x: .8
    MDTextField:
        id: id_user_password
        hint_text: 'Password'
        pos_hint: {'center_x': .5,'center_y': .5}
        current_hint_text_color: 0,0,0,1
        size_hint_x: .8
        password: True
    MDLabel: 
        id: id_loading
        text:'Loading...'
        pos_hint: {'center_y': .4}
        font_style: 'H5'
        halign: 'center'
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        opacity: 0
    MDRaisedButton:
        text: 'Log in'
        pos_hint: {'center_x': .5, 'center_y': .2}
        size_hint_x:.5
        on_release: root.event_sign_in()
    MDRaisedButton:
        text: 'Log in with recognize face'
        pos_hint: {'center_x': .5, 'center_y': .3}
        size_hint_x:.5
        on_press: root.event_sign_in_with_face()
        
